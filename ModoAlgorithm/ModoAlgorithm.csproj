<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

	<PropertyGroup>
		<VersionPrefix>0.0.1</VersionPrefix>
		<Description>A generic algorithm to generate all nondominated solutions for multiobjective discrete optimization (MODO) problems with any number of objective functions.</Description>
		<PackageProjectUrl>https://ugur-arikan.github.io/ModoAlgorithm/</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Ugur-Arikan/ModoAlgorithm</RepositoryUrl>
		<Authors>ugur-arikan</Authors>
		<PackageTags>multiobjective;discrete;optimization;modo;nondominated;pareto;frontier</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

  <ItemGroup>
    <PackageReference Include="MathModel" Version="0.0.1" />
    <PackageReference Include="OptRes" Version="6.0.0" />
  </ItemGroup>

</Project>
